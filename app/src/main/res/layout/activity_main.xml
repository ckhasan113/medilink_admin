                android:layout_height="wrap_content" >

                <androidx.cardview.widget.CardView
                    android:id="@+id/doctor_list_admin_panel"
                    android:layout_marginRight="3pt"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="1dp"
                    android:layout_width="40pt"
                    android:layout_height="56pt"
                    app:cardCornerRadius="10dp"
                    android:layout_weight="1"
                    >

                    <LinearLayout
                        android:layout_margin="8dp"
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        >


                        <ImageView
                            android:src="@drawable/doctor_info"
                            android:layout_width="45dp"
                            android:layout_height="40dp" />

                        <View
                            android:layout_marginTop="8dp"
                            android:layout_width="35dp"
                            android:layout_height="1dp"
                            android:background="#FE5237"
                            />



                    </LinearLayout>

                    <TextView
                        android:layout_marginLeft="8dp"
                        android:layout_marginBottom="4dp"
                        android:layout_marginTop="8dp"
                        android:fontFamily="sans-serif"
                        android:textColor="#000"
                        android:text="Doctor List\n"
                        android:textSize="14sp"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="bottom"
                        />

                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:id="@+id/hospital__list_admin_panel"
                    android:layout_marginRight="3pt"
                    android:layout_marginLeft="1dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginBottom="1dp"
                    android:layout_width="40pt"
                    android:layout_height="56pt"
                    app:cardCornerRadius="10dp"
                    android:layout_weight="1"
                    >

                    <LinearLayout
                        android:layout_margin="8dp"
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        >


                        <ImageView
                            android:src="@drawable/hospital_info"
                            android:layout_width="45dp"
                            android:layout_height="40dp" />

                        <View
                            android:layout_marginTop="8dp"
                            android:layout_width="35dp"
                            android:layout_height="1dp"
                            android:background="#FE5237"
                            />

